<main>

<div class="container">
    <h1 class="mt-5 text-center">Cinemas Page</h1>

    <div class="row mb-2">
        <% for (var i = 0; i < cinemas.length; i++) {%>
       
        <div class="col-md-6">
          <div class="row g-0 border rounded overflow-hidden flex-md-row mb-4 shadow-sm h-md-250 position-relative">
            <div class="col p-4 d-flex flex-column position-static">
              <strong class="d-inline-block mb-2 text-primary-emphasis">No Of Screen <%= cinemas[i].noOfScreen %></strong>
              <h3 class="mb-0"><%= cinemas[i].name %></h3>
              <div class="mb-1 text-body-secondary"><%= cinemas[i].phone %></div>
              <p class="card-text mb-auto"><%= cinemas[i].address %></p>
              <a href="#" class="icon-link gap-1 icon-link-hover stretched-link">
                Continue reading
                <svg class="bi"><use xlink:href="#chevron-right"/></svg>
              </a>
            </div>
            <div class="col-6 d-lg-block">
              <img src="<%= cinemas[i].cover %>" class="img-fluid" alt="" />
            </div>
          </div>
        </div>
        <%}%>
      </div>
    </div>

